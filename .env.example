# Supabase - Obrigatório para autenticação e banco de dados
VITE_SUPABASE_URL=https://<seu-projeto>.supabase.co
VITE_SUPABASE_ANON_KEY=ANON_KEY....

# Supabase - Chave de serviço (usada apenas em funções Edge)
SUPABASE_SERVICE_ROLE_KEY=SERVICE_ROLE_KEY....
SUPABASE_PROJECT_REF=seu-ref-projeto

# Resend - Serviço de envio de emails (usado nas funções Edge)
VITE_RESEND_API_KEY=RESEND_API_KEY....
RESEND_API_KEY=RESEND_API_KEY....

# Stripe - Obrigatório para processamento de pagamentos
VITE_STRIPE_PUBLIC_KEY=pk_test_...

# Sentry - Opcional para monitoramento de erros
VITE_SENTRY_DSN=https://...

# Google Analytics - Opcional para análise de uso
VITE_GA_MEASUREMENT_ID=G-...

# Vercel - Para deploy automático via GitHub Actions
# Obtenha estes valores executando 'npx vercel link' ou no dashboard do Vercel
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_vercel_org_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here
